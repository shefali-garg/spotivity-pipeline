pipeline
{
agent any
stages
{
    stage('Build')
    {
    steps{
        bat "mvn clean"
        }
    }
    
     stage('Test')
    {
     steps{
     bat "mvn test"
        echo"test"
        echo"testing"
        }
    }
    stage('Compile')
    {
     steps{
     bat "mvn compile"
        }
    }
      stage('Deploy')
    {
       steps{ 
       echo "deploy"
    }
    }}
    
}
